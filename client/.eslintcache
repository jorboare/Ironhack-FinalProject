.state.result &&\n                            <>\n                                <h5>Resultado</h5>\n                                {this.state.result.map(elm =>\n                                    <FriendCard key={elm._id} friendId={elm._id} />\n                                )}\n\n                            </>\n                        }\n                    </Row>\n                    <Row>\n                        {this.state.userFriends.map((elm, idx) => <FriendCard key={idx} friendId={elm} />)}\n                    </Row>\n                </Container>\n            </>\n\n\n\n        )\n    }\n}","/Users/Jordi/Desktop/IRONHACK/BOOTCAMP/FINAL-PROJECT/Ironhack-FinalProject/client/src/components/pages/profile/followed-users/friend-card/Friend-card.js",[],"/Users/Jordi/Desktop/IRONHACK/BOOTCAMP/FINAL-PROJECT/Ironhack-FinalProject/client/src/components/pages/friend-profile/profile-page/profile.js",["120"],"/Users/Jordi/Desktop/IRONHACK/BOOTCAMP/FINAL-PROJECT/Ironhack-FinalProject/client/src/components/pages/friend-profile/profile-navbar/profile-Navbar.js",[],"/Users/Jordi/Desktop/IRONHACK/BOOTCAMP/FINAL-PROJECT/Ironhack-FinalProject/client/src/components/pages/friend-profile/profile-header/profile-header.js",[],"/Users/Jordi/Desktop/IRONHACK/BOOTCAMP/FINAL-PROJECT/Ironhack-FinalProject/client/src/components/pages/friend-profile/profile-feed/Recipe-card.js",[],"/Users/Jordi/Desktop/IRONHACK/BOOTCAMP/FINAL-PROJECT/Ironhack-FinalProject/client/src/components/pages/profile/profile-page/discover/Discover.js",[],"/Users/Jordi/Desktop/IRONHACK/BOOTCAMP/FINAL-PROJECT/Ironhack-FinalProject/client/src/components/pages/profile/profile-page/my-recipes/My-Recipes.js",[],"/Users/Jordi/Desktop/IRONHACK/BOOTCAMP/FINAL-PROJECT/Ironhack-FinalProject/client/src/components/pages/profile/profile-page/saved-recipes/Saved-recipes.js",["121"],"import React, { Component } from 'react'\nimport RecipeCard from './Fav-Recipe-card'\nimport RecipesService from '../../../../../service/recipe